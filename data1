<!-- D. 화면 내부 전용 스크립트 -->
<script type="text/javascript">
	$(function() {
		"use strict";

		var songId = '37229121';
		var snsGate = '';

		if(snsGate == 'Y'){
			//공유를 통해서 진입시
			if(MELON.WEBSVC.POC.getPocId() == 'IT10' || MELON.WEBSVC.POC.isIPad()){
				//아이패드인 경우는 아이패드 앱의 곡상세로 랜딩
				MELON.WEBSVC.POC.link.ipadAppLaunch('melonipad://details/song?cid='+songId);
			}
		}

		var $doc = $(document),
				WEBSVC = MELON.WEBSVC,
				PBPGN = MELON.PBPGN,
				addComma = WEBSVC.number.addComma;
		//DJ 좋아요..
		WEBSVC.DJCollection.init();

		var lyricItemYN = 'Y';
		var adultFlg = '0';

		if(lyricItemYN == 'N'){
			$("#lyricPrintReportBtn").hide();
		}

		// 가사가 존재 && 19금 곡일 경우, 회원 API 호출
		if(lyricItemYN == 'Y' && adultFlg  == '1'){
            $("#lyricPrintReportBtn").hide();
            // 로그인
			if(MELON.WEBSVC.POC.login.isMelonLogin()) {
				//2022.08.17 회원 쿠키 사용 제거
				var memberInfo = svcMember.checkAgeAuthByKey(getMemberKey(), MELON.WEBSVC.POC.getPocId());
				// 성인인증 완료한 경우만 가사정보 호출
				if(memberInfo.AUTHYN == 'Y' && memberInfo.ADULTAUTHFLG == '1'){
					$.ajax({
						type:"get",
						url: '/song/lyricInfo.json',
						dataType:"json",
						async:true,
						data : {
							songId: '37229121'
						},
						success:function(obj){
							$(".lyric").html(obj.lyric);
							$("#lyricPrintReportBtn").show();
						},
						error: function(request,error) {
						}
					});
				}

				console.log('memberInfo ::: ', memberInfo);

				// adultAuthFlg : 19금컨텐츠 이용을 위한 성인인증 플래그
				// 0: 19금 컨텐츠 이용 불가 ( 본인인증+19세미만 케이스 [인증1년 경과 무관] )
				// 1: 이용가능 ( 본인인증+인증 1년미만+19세이상 )
				// 2: 본인인증 필요 ( 미본인인증 케이스 )
				// 3: 재인증 필요 ( 본인인증+인증 1년 경과+19세이상)

				// 기존 memberAuthFlg 기준  >> 1 : 성인 - 본인인증 O, 성인인증 O
				if(memberInfo.AUTHYN == 'Y' && memberInfo.ADULTAUTHFLG == '1'){
					var lyricHtml  = '<div class="lyric" id="d_video_summary"></div>';
					lyricHtml += '<button type="button" title="Shopper 가사 더보기" class="button_more arrow_d" data-control="expose" data-expose-type="more" data-expose-target="#d_video_summary">';
					lyricHtml += '	<span class="text">펼치기</span>';
					lyricHtml += '	<i class="button_icons etc arrow_d"></i>';
					lyricHtml += '</button>';
				}
				// 기존 memberAuthFlg 기준  >> 0 : 미성년 - 본인인증 O, 성인인증 X
				else if(memberInfo.ADULTAUTHFLG == '0'){
					var lyricHtml  = '<div class="lyric_none">';
					lyricHtml += '	<span class="icon_point_green"></span>청소년 보호법에 따라 19세 미만의 청소년이 이용할 수 없는 콘텐츠 입니다.';
					lyricHtml += '</div>';
				}
				// 기존 memberAuthFlg 기준  >> (2 : 성인 - 본인인증 X, 성인인증 X), (3 : 재인증 필요 (본인인증 + 인증1년경과 + 19세 이상))
				else{
					var lyricHtml = '<div class="lyric_none">';
					lyricHtml += '	<span class="icon_point_green"></span>청소년 보호법에 따라 성인 인증이 필요한 콘텐츠 입니다. 성인 인증 후 이용해 주세요.';
					lyricHtml += '	<button type="button" title="성인 인증" class="btn_base02 adult_register"><span class="odd_span"><span class="even_span">성인 인증</span></span></button>';
					lyricHtml += '</div>';
				}
			// 비로그인
			}else{
				var lyricHtml = '<div class="lyric_none">';
				lyricHtml += '	<span class="icon_point_green"></span>청소년 보호법에 따라 성인 인증이 필요한 콘텐츠 입니다. 성인 인증 후 이용해 주세요.';
				lyricHtml += '	<button type="button" title="성인 인증" class="btn_base02 adult_register"><span class="odd_span"><span class="even_span">성인 인증</span></span></button>';
				lyricHtml += '</div>';
			}

			// 가사 셋팅
			if(lyricHtml != null) {
				$('#lyricArea').html(lyricHtml);
			}
		}

		try  {

			/* 좋아요 셋팅부 */
			var LIKE_SET = {
				likeCnt  : "<span class=\"odd_span\">{TXT}</span>\n<span id=\"d_like_count\" class=\"cnt\">\n<span class=\"none\">총건수</span>\n{CNT}</span>",
				likeObj  : $('#btnLike'),
				likeAttr : 'data-song-no',
				likeUrl  : '/commonlike/getSongLike.json'
			};

			/* 좋아요 공통부 */
			var contsIdList = LIKE_SET['likeObj'].map(function() { return $(this).attr(LIKE_SET['likeAttr']); }).get().join(',');
			var tmpl, title;
			if ( contsIdList === '' || contsIdList === null  ) { return; }
			$.get(LIKE_SET['likeUrl'], { contsIds : contsIdList }, function(data) {
				var robj;
				$.each(data.contsLike, function(i, v) {
					var SUMMCNT = MELON.WEBSVC.number.addComma(v.SUMMCNT);
					robj  = $('#btnLike');
					tmpl  = LIKE_SET['likeCnt'];
					title = robj.attr('title').split(' 좋아요');
					/*
                    robj[v.LIKEYN == 'Y' ? 'addClass' : 'removeClass']('on').attr('title', title[0] + (v.LIKEYN == 'Y' ? ' 좋아요 취소' : ' 좋아요'));
                    robj.html(
                        tmpl.replace(/\{TXT\}/g, (v.LIKEYN == 'Y' ? '좋아요 취소' : '좋아요')).replace(/\{CNT\}/g, SUMMCNT)
                    );
                    */
					if(v.LIKEYN == 'Y') {
						$('.section_info').find('.entry .button_etc.like').addClass('on').attr('title', title[0] + ' 좋아요 취소');
						$('.section_info').find('.entry .button_etc.like > .odd_span').text('좋아요 취소');
					}
					else {
						$('.section_info').find('.entry .button_etc.like').removeClass('on').attr('title', title[0] + ' 좋아요');
						$('.section_info').find('.entry .button_etc.like > .odd_span').text('좋아요');
					}
					$('.section_info').find('.entry .button_etc.like > .cnt').text(SUMMCNT);
				});
			}).done(function(){
			});

		} catch (e) {
			console.log(e);
		}

		$.ajax({
			type:"get",
			url: '/song/songReviewCnt.json',
			dataType:"json",
			async:true,
			data : {
				songId: '37229121'
			},
			success:function(obj){
				var revCnt = MELON.WEBSVC.number.addComma(obj.reviewValidCmtCnt);
				if(revCnt > 999999) revCnt='999,999+';
				$("#revCnt").text(revCnt+"개");
			},
			error: function(request,error) {
			}
		});

		
		try  {

			// DJ플레이리스트의 좋아요 처리

			/* 좋아요 셋팅부 */
			var LIKE_SET = {
				likeCnt  : '<span class="odd_span">{TXT}</span> <span class="cnt"> <span class="none">총건수</span> {CNT} </span>',
				likeObj  : $('.btn_djplylst_like'),
				likeAttr : 'data-djcol-no',
				likeUrl  : '/dj/common/djcommon_listLikeCnt.json'
			};

			/* 좋아요 공통부 */
			var contsIdList = LIKE_SET['likeObj'].map(function() { return $(this).attr(LIKE_SET['likeAttr']); }).get().join(',');

			var tmpl, title;
			$.get(LIKE_SET['likeUrl'], { contsIds : contsIdList }, function(data) {
				var robj;
				$.each(data.contsLike, function(i, v) {
					var SUMMCNT = MELON.WEBSVC.number.addComma(v.SUMMCNT);
					robj  = LIKE_SET['likeObj'].eq(i);
					tmpl  = LIKE_SET['likeCnt'];
					title = robj.attr('title').split(' 좋아요');
					robj[v.LIKEYN == 'Y' ? 'addClass' : 'removeClass']('on').attr('title', title[0] + (v.LIKEYN == 'Y' ? ' 좋아요 취소' : ' 좋아요'));

					robj.html(
							tmpl.replace(/\{TXT\}/g, (v.LIKEYN == 'Y' ? '좋아요 취소' : '좋아요')).replace(/\{CNT\}/g, SUMMCNT)
					);
				});
			}).done(function(){
			});

		} catch (e) {
			console.log(e);
		}
		

		// 뮤직 비디오 탭
		$('.section_mv').toggleSlider({
			type:'count-on',
			selectors: {
				tabs: '>div.wrap_page>div.page>span.wrap_btn>a',
				contents: '>div.wrap_list_mv>ul',
				nowpages: '>div.wrap_page>div.page>span.page_num>strong',
				totalpages: '>div.wrap_page>div.page>span.page_num>span'
			}
		});

		$('.section_info').each(function() {
			if ($(this).find('.song_name').height() > 30) {
				$(this).find('.song_name').addClass('multi');
			}
		});

		//melon.show.moreButton('d_video_summary', 240);

		// 좋아요한 사람
		$('.section_intst').ajaxToggle({
			type:'count-on',
			selectors: {
				// 버튼 영역 [d_btn]은 공통이기 때문에 특정 클래스를 준다.
				tabs: 'a.withLikeBtnTmp',
				nowpages: 'strong.d_now_page',
				// 토탈 페이지 DP 영역
				totalpages: 'span.d_total_page',
				// AJAX 호출후 InnerHtml 해줘야할 영역
				contents: 'div.wrap_list_intst',
				//orderlayer : 'a.orderClass' // 정렬 Layer
			}
		});

		// 좋아요한 사람 정렬 클릭
		$('a.orderClass').on('click', function(e){
			$('.list_sort').find(">li").each(function() {
				$(this).removeClass("on");
			})
			$(this).parent().addClass("on");
			var inputId = this.id;
			$("a.withLikeBtnTmp").each(function() {
				$(this).attr("data-orderby-type",inputId);
			});
		});

		// 프린트
		$('.d_print').on('click', function(){
			WEBSVC.openPopup('/song/popup/lyricPrint.htm?songId=37229121', 560, 616, 'no');
		});

		// 가사오류신고
		$('.d_report').on('click', function(){
			if(!MELON.WEBSVC.POC.login.isMelonLogin()) {
				MELON.WEBSVC.POC.login.loginPopupLayerd();
				//alert("로그인 후 사용 가능합니다.")
				return;
			}

			WEBSVC.openPopup('/song/popup/lyricInsertError.htm?songId=37229121', 560, 590);
		});

		// 가사등록하기
		$('.d_register').on('click', function(){
			if(!MELON.WEBSVC.POC.login.isMelonLogin()) {
				MELON.WEBSVC.POC.login.loginPopupLayerd();
				//alert("로그인 후 사용 가능합니다.")
				return;
			}
			WEBSVC.openPopup('/song/popup/lyricRegist.htm?songId=37229121', 560, 590);
		});

		// 성인인증
		$('.adult_register').on('click', function(){
			if(!MELON.WEBSVC.POC.login.isMelonLogin()) {
				MELON.WEBSVC.POC.login.loginPopupLayerd();
				//alert("로그인 후 사용 가능합니다.")
				return;
			}
			// niceAuthPop('nineteen','Y','','',true,'00');
			goCertification();
		});

		function goCertification(){
			var width = 600;
			var height = 464;

			var pocId = MELON.WEBSVC.POC.getPocId();
			var realnameUrl = "https://member.melon.com/ageauth/main_inform.htm?cpId=" + pocId + "&viewType=nineteen&footer=off&callback=";
			var callbackUrl = "https://www.melon.com/album/selfCertCallback.htm";

			realnameUrl = realnameUrl + callbackUrl;

			var urlOpt = "scrollbars=yes, resizable=yes,location=no, width="+width+",height="+height+", left=20, top=20";
			window.open(realnameUrl, "_REALNAME_AUTHENTICATION_POP","app_,"+urlOpt);
		}

		/************ SNS *****************/
		$("#songFacebook").click(function(){
			$.songName = "Shopper";
			$.albumName = "The Winning";
			$.artistName = "아이유";
			$.imgUrl = $("#d_song_org img").attr("src");
			$.menuId = $(this).attr("data-menuId");
			melon.sns.facebook({
				type 		: "son",
				contsId 	: "37229121",
				songName 	: $.songName,
				albumName	: $.albumName,
				artistName  : $.artistName,
				imgUrl      : $.imgUrl,
				menuId		: $.menuId
			});
		});
		//트위터
		$("#songTwitter").click(function(){
			$.songName = "Shopper";
			$.albumName = "The Winning";
			$.artistName = "아이유";
			$.menuId = $(this).attr("data-menuId");
			melon.sns.twitter({
				type 		: "son",
				contsId 	: "37229121",
				songName 	: $.songName,
				albumName	: $.albumName,
				artistName  : $.artistName,
				menuId		: $.menuId
			});
		});

		$('#btnLike').on('click', function(){
			var $btn = $(this),
					songNo = $btn.attr('data-song-no'),
					menuId = $btn.attr('data-song-menuId'),
					title = $btn.attr('title').split(' 좋아요'),
					isLike = $btn.hasClass('on'), doLike = !isLike,
					addComma = WEBSVC.number.addComma,
					$target = (function(){
						var targetId = $btn.attr('data-target-id');
						if(!targetId) { return $btn; }
						targetId = targetId.substr(0, 1) === '#' ? targetId : '#' + targetId;
						return $(targetId);
					})(),
					defer, event;

			$btn.trigger((event = $.Event('likebefore')), [songNo, doLike]);
			if(event.isDefaultPrevented()){ return; }

			if(isLike) {
				defer = WEBSVC.SongList.dislike(songNo,menuId)
			} else {
				defer = WEBSVC.SongList.like(songNo,menuId);
			}
			defer.done(function(json) {
				var tmpl = '';
				if(json.result === true) {
					$btn.trigger((event = $.Event('likechanged')), [songNo, title[0], doLike, json.data.SUMMCNT.toString().replace(/(\d)(?=(?:\d{3})+$)/g, "$1,")]);
					if(event.isDefaultPrevented()){ return; }

					$btn[doLike ? 'addClass' : 'removeClass']('on').attr('title', title[0] + (doLike ? ' 좋아요 취소' : ' 좋아요'));

					$target.html(json.data.SUMMCNT.toString().replace(/(\d)(?=(?:\d{3})+$)/g, "$1,"));

					// 131114_수정
					doLike && WEBSVC.alert2('좋아요 반영되었습니다.',{opener :$btn, removeOnClose:true});//140113_수정
					//loadTopLike();
					!doLike && WEBSVC.alert2('좋아요 취소되었습니다.',{opener :$btn, removeOnClose:true});

					if(json.data.SUMMCNT > 0) {
						$('.section_intst').attr('style', 'display:');
					}
					else {
						$('.section_intst').attr('style', 'display:none');
					}

					loadBottomLike();
					loadLikeUserTotCount();
				} else {
					alert(json.errorMessage);
					//토큰 유효성체크
					if(json.tockenValid != undefined && json.tockenValid == false){
						var pocId = MELON.WEBSVC.POC.getPocId();
						if('WP42' == pocId){
							try {
								MelonAPI.window("forceLogout", "");
							} catch(e) {
							}
						}else{
							location.href = json.returnUrl;
						}
					}

				}
			}).fail(function(msg){
				alert(msg);
			});
		});

		function loadTopLike() {
			$.ajax({
				url: '/song/songTopLikeUser.htm',
				data: {
					contsId : '37229121',
					viewPage : '1'
				},
				async : false
			}).done(function(html) {
				$('#songTopLikeUserLayer').html(html);
			});
		}

		function loadBottomLike() {
			$.ajax({
				url: '/song/withLikeUserSong.htm',
				data: {
					contsId : '37229121',
					viewPage : '1'
				},
				async : false
			}).done(function(html) {
				$('.wrap_list_intst').html(html);
			});
		}

		loadBottomLike();

		function loadLikeUserTotCount() {
			$.ajax({
				url: '/song/withLikeUserSong.json',
				dataType:"json",
				data: {
					contsId : '37229121',
					viewPage : '1'
				},
				async : false
			}).done(function(json) {
				if(json.withLikeListTotCountPg == 0)
					$("#bottomListUserLayer").hide();
				else
					$("#bottomListUserLayer").show();
				$("#withLikeBtnTmp1").attr("data-limit-count",json.withLikeListTotCountPg);
				$("#withLikeBtnTmp2").attr("data-limit-count",json.withLikeListTotCountPg);
				$('div.section_intst').ajaxToggle({
					type:'count-on',
					selectors: {
						// 버튼 영역 [d_btn]은 공통이기 때문에 특정 클래스를 준다.
						tabs: 'a.withLikeBtnTmp',
						nowpages: 'strong.d_now_page',
						// 토탈 페이지 DP 영역
						totalpages: 'span.d_total_page',
						// AJAX 호출후 InnerHtml 해줘야할 영역
						contents: 'div.wrap_list_intst',
						//orderlayer : 'a.orderClass' // 정렬 Layer
					}
				});
			});
		}

		loadLikeUserTotCount();

		function loadMvInfo() {
			$.ajax({
				url: '/song/songMvInfo.htm',
				data: {
					songId : '37229121'
				},
				async : false
			}).done(function(html) {
				$('.service_list_video').html(html);
			});
		}

		//loadMvInfo();

		// 관련비디오 탭
		$('.section_movie').toggleSlider({
			type:'count-on',
			selectors: {
				tabs: '>div.page>span.wrap_btn>a',
				contents: '>div.service_list_video>ul',
				nowpages: '>div.page>span.page_num>strong',
				totalpages: '>div.page>span.page_num>span'
			}
		});

	});

	function goReview(){
		var position = $("#section_cmt").offset();
		$("html, body").animate({scrollTop : position.top}, 0);
	}
</script>
